<?xml version="1.0" encoding="UTF-8"?>


<mapper namespace="memberMapper">
	<!-- 회원가입 -->
	<insert id="register">
		INSERT INTO userInfo(userType,userName,userId,password,birthday,gender,email,contact,address)
		VALUES(#{userType},#{userName},#{userId}, #{password},#{birthday}, #{gender}, #{email},#{contact},#{address})
	</insert>
	
	<!-- 로그인 -->
	<select id="login" resultType="com.deu.Amall.domain.MemberVO">
		select userId,password
		from userInfo
		where userId = #{userId}
		and password= #{password}
	
	</select>
</mapper>